{
    "name": "igroup/ig-history",
    "type": "typo3-cms-extension",
    "description": "Shows the history as a timeline.",
    "authors": [
        {
            "name": "Marvin Rudolph",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "Igroup\\IgHistory\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Igroup\\IgHistory\\Tests\\": "Tests"
        }
    },
    "replace": {
        "ig_history": "self.version",
        "typo3-ter/ig-history": "self.version"
    }
}